package uk.ac.liv.ffmpeg.libavcodec.raw;

import uk.ac.liv.ffmpeg.libavutil.PixFmt.PixelFormat;

public class PixelFormatTag {
	
	PixelFormat pix_fmt;
	int fourcc;	
	
	
	public PixelFormatTag(PixelFormat pix_fmt, int fourcc) {
		super();
		this.pix_fmt = pix_fmt;
		this.fourcc = fourcc;
	}
	
	public PixelFormat get_pix_fmt() {
		return pix_fmt;
	}
	
	public void set_pix_fmt(PixelFormat pix_fmt) {
		this.pix_fmt = pix_fmt;
	}
	
	public int get_fourcc() {
		return fourcc;
	}
	
	public void set_fourcc(int fourcc) {
		this.fourcc = fourcc;
	}
	
	

}
