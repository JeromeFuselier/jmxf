package uk.ac.liv.ffmpeg.libavformat.img2;

import uk.ac.liv.ffmpeg.libavutil.AVClass;

public class VideoData {

	
	AVClass av_class;  /**< Class for private options. */
    int img_first;
    int img_last;
    int img_number;
    int img_count;
    int is_pipe;
    int split_planes;  /**< use independent file for each Y, U, V plane */
    String path;
    char pixel_format;     /**< Set by a private option. */
    char video_size;       /**< Set by a private option. */
    char framerate;        /**< Set by a private option. */
    int loop;
    
	public AVClass get_class() {
		return av_class;
	}
	
	public void set_class(AVClass av_class) {
		this.av_class = av_class;
	}

	public int get_img_first() {
		return img_first;
	}

	public void set_img_first(int img_first) {
		this.img_first = img_first;
	}

	public int get_img_last() {
		return img_last;
	}

	public void set_img_last(int img_last) {
		this.img_last = img_last;
	}

	public int get_img_number() {
		return img_number;
	}

	public void set_img_number(int img_number) {
		this.img_number = img_number;
	}

	public int get_img_count() {
		return img_count;
	}

	public void set_img_count(int img_count) {
		this.img_count = img_count;
	}

	public int get_is_pipe() {
		return is_pipe;
	}

	public void set_is_pipe(int is_pipe) {
		this.is_pipe = is_pipe;
	}

	public int get_split_planes() {
		return split_planes;
	}

	public void set_split_planes(int split_planes) {
		this.split_planes = split_planes;
	}

	public String get_path() {
		return path;
	}

	public void set_path(String path) {
		this.path = path;
	}

	public char get_pixel_format() {
		return pixel_format;
	}

	public void set_pixel_format(char pixel_format) {
		this.pixel_format = pixel_format;
	}

	public char get_video_size() {
		return video_size;
	}

	public void set_video_size(char video_size) {
		this.video_size = video_size;
	}

	public char get_framerate() {
		return framerate;
	}

	public void set_framerate(char framerate) {
		this.framerate = framerate;
	}

	public int get_loop() {
		return loop;
	}

	public void set_loop(int loop) {
		this.loop = loop;
	}
	

}
