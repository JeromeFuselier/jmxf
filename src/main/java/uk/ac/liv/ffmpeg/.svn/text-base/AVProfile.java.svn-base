package uk.ac.liv.ffmpeg;

public class AVProfile {
	
	
	int profile;
	String name; // short name for the profile
	
	
	public AVProfile(int profile, String name) {
		super();
		this.profile = profile;
		this.name = name;
	}


	public int get_profile() {
		return this.profile;
	}
	
	public String get_name() {
		return this.name;
	}
	
	
	
}
