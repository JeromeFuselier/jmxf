package uk.ac.liv.ffmpeg;


	/* select an input stream for an output stream */
public class AVStreamMap {
	
    int file_index;
    int stream_index;
    int sync_file_index;
    int sync_stream_index;
    
    
	public int get_file_index() {
		return file_index;
	}
	
	public void set_file_index(int file_index) {
		this.file_index = file_index;
	}
	
	public int get_stream_index() {
		return stream_index;
	}
	
	public void set_stream_index(int stream_index) {
		this.stream_index = stream_index;
	}
	
	public int get_sync_file_index() {
		return sync_file_index;
	}
	
	public void set_sync_file_index(int sync_file_index) {
		this.sync_file_index = sync_file_index;
	}
	
	public int get_sync_stream_index() {
		return sync_stream_index;
	}
	
	public void set_sync_stream_index(int sync_stream_index) {
		this.sync_stream_index = sync_stream_index;
	}
    
    
}
