package uk.ac.liv.ffmpeg;

import uk.ac.liv.ffmpeg.libavcodec.AVCodec.AVPacketSideDataType;

public class AVSideData {
	
	
	
	short [] data;
    int     size;
    AVPacketSideDataType type = AVPacketSideDataType.AV_PKT_DATA_PALETTE;
    
	public short [] get_data() {
		return data;
	}
	public void set_data(short [] data) {
		this.data = data;
	}
	public int get_size() {
		return size;
	}
	public void set_size(int size) {
		this.size = size;
	}
	public AVPacketSideDataType get_type() {
		return type;
	}
	public void set_type(AVPacketSideDataType type) {
		this.type = type;
	}
    
    
}
